#include <iostream>
#include <queue>
#include <vector>
#include <algorithm>
using namespace std;

int main() {
    ios::sync_with_stdio(false);
    cin.tie(nullptr); cout.tie(nullptr);
    
    priority_queue<int> maxHeap;

    int N; int insert;
    
    cin>>N;

    for(int i=0;i<N;i++){
    cin>>insert;
        if(insert>=1){
            maxHeap.push(insert);
        }
        if(insert==0){
            if(maxHeap.empty())cout<< "0"<<" ";
            else {
             cout<<maxHeap.top()<<" ";
            maxHeap.pop();   
            }
        }
    }

    return 0;
}