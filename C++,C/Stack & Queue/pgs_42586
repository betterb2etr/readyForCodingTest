#include <iostream>
#include <algorithm>
#include <string>
#include <vector>
#include <queue>
using namespace std;

vector<int> solution(vector<int> progresses, vector<int> speeds) {

    int now=0;
    vector<int> answer;
    queue<long> status;
    queue<long> velocity;

    for(int i : progresses){
       status.push(i);
    }
      for(int i : speeds){
       velocity.push(i);
    }


    int idx = 0;
    int cnt= 0 ;


    int answerIdx =0;
    int turn=0;

    while(!status.empty() && !velocity.empty()){
        ++turn;
;        while( (status.front() + turn*velocity.front() ) >= 100 ) {
            status.pop(); velocity.pop();
            cnt++;
        }

        if (cnt > 0 ){
            cout<<answerIdx; cout<< "\n";


                    answer.push_back(cnt);  // âœ…
        cnt = 0;
        }
    }


    return answer;
}